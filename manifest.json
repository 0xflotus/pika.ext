{
  "manifest_version": 2,
  "name": "Pika.dev Search engine",
  "description": "Adds a search engine that searches pika.dev",
  "version": "1.0",
  "applications": {
    "gecko": {
      "strict_min_version": "55"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Pika",
      "search_url": "https://www.pika.dev/search?q={searchTerms}",
      "keyword": "pika",
      "is_default": false,
      "encoding": "UTF-8"
    }
  }
}
